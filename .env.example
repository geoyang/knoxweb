# Kizu Web Configuration
# Copy this file to .env.development and fill in your values

# ===========================================
# Supabase (primary connection)
# ===========================================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# ===========================================
# Environment Switcher (dev/prod toggle in Login)
# ===========================================
VITE_DEV_SUPABASE_URL=https://your-dev-project.supabase.co
VITE_DEV_SUPABASE_ANON_KEY=your-dev-anon-key
VITE_PROD_SUPABASE_URL=https://your-prod-project.supabase.co
VITE_PROD_SUPABASE_ANON_KEY=your-prod-anon-key

# ===========================================
# ImageKit
# ===========================================
VITE_IMAGEKIT_KEY=your-imagekit-private-key
VITE_IMAGEKIT_URL=https://ik.imagekit.io/your-endpoint

# ===========================================
# Firebase Cloud Messaging
# ===========================================
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id
VITE_FIREBASE_VAPID_KEY=your-vapid-key

# ===========================================
# Kizu AI API
# ===========================================
VITE_AI_API_URL=http://localhost:8000
